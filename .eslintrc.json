{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "rules": {
    "no-undef": "warn",
    "no-console": "off",
    "no-const-assign": "error",
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-misleading-character-class": 0,
    "implicit-arrow-linebreak": 0,
    "semi": ["error", "always"],
    "space-in-parens": "off"
  }
}
